{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "qrcode-marker",
  "title": "qrcode marker",
  "description": "generate a qrcode and open with raycast",
  "icon": "qrcode.png",
  "author": "o4liangdu",
  "categories": [
    "Applications"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "index",
      "title": "generate qrcode",
      "subtitle": "qrcode",
      "description": "generate a qrcode and open with raycast",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.49.3",
    "qrcode": "^1.5.1"
  },
  "devDependencies": {
    "@raycast/eslint-config": "1.0.5",
    "@types/node": "18.8.3",
    "@types/qrcode": "^1.5.0",
    "@types/react": "18.0.9",
    "eslint": "^7.32.0",
    "prettier": "^2.5.1",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "npx @raycast/api@latest publish"
  }
}
